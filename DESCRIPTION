Package: partialised
Type: Package
Title: Partialised Functions
Version: 0.1.0
Authors@R: 
    person(
        given = "Mizuki", 
        family = "Uchida", 
        email = "uchidamizuki@vivaldi.net", 
        role = c("aut", "cre")
